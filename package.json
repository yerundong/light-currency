{
  "name": "light-currency",
  "version": "1.0.33",
  "description": "A small, lightweight javascript library for working with currency values.",
  "author": "yerundong",
  "license": "MIT",
  "homepage": "https://github.com/yerundong/light-currency.git",
  "main": "dist/standard/light-currency.cjs.js",
  "module": "dist/standard/light-currency.es.js",
  "browser": "dist/standard/light-currency.min.js",
  "scripts": {
    "rollup": "rollup --config config/rollup.config.js ",
    "rollup-ie": "rollup --config config/rollup.config.ie.js ",
    "babel": "npx babel src/light-currency.js -o src/light-currency.babel.js",
    "babel-ie": "npx babel src/light-currency.js -o src/light-currency.babel.ie.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yerundong/light-currency.git"
  },
  "keywords": [
    "light-currency",
    "currency",
    "format",
    "money",
    "accounting",
    "number",
    "parse",
    "precision",
    "decimal"
  ],
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "core-js": "^3.18.0",
    "rollup": "^2.57.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4"
  }
}